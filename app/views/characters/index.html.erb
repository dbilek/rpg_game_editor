<div class="col-md-12">
  <div class="col-md-4 col-md-offset-4">
    <h2>Your characters</h2>
      <ul>
        <% @characters.each do |character| %>
          <li><%= image_tag character.avatar.thumb %>
          <%= link_to character.name, character_path(character) %></li>
        <% end %>
      </ul>
  </div>
</div>